a = input('Введите свой вес, кг:')
b = input('Введите свой рост, см:')
c = float(a)
d = float(b)
e = d / ((c * 0.01)**2)
print("Ваш имт:", round(e))
l = ['=', '=', '=', '=', '=', '=', '=', '=', '=', '=', '=', '=',]
h = round((e-20)/((56-20)/len(l)))
print(h)
l[h] = '|'
m = ' '.join(l)
print(20, m, 56)